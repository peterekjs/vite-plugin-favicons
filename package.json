{
  "name": "@peterek/vite-plugin-favicons",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Simple Vite plugin to generate favicons from source image. Nothing more, nothing less.",
  "author": {
    "name": "Jiri Peterek",
    "email": "jiri@peterek.net"
  },
  "keywords": [
    "vite-plugin",
    "favicon",
    "favicons",
    "image",
    "ico",
    "icon",
    "generate",
    "generator"
  ],
  "files": [
    "index.cjs",
    "index.mjs",
    "index.d.ts",
    "hooks/*",
    "lib/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": ["./index.d.ts"]
    }
  },
  "types": "index.d.ts",
  "dependencies": {
    "favicons": "^7.1.5"
  },
  "devDependencies": {
    "@types/favicons": "^7.0.0"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterekjs/vite-plugin-favicons.git"
  },
  "bugs": {
    "url": "https://github.com/peterekjs/vite-plugin-favicons/issues"
  },
  "homepage": "https://github.com/peterekjs/vite-plugin-favicons#readme"
}
